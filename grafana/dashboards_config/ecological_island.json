          "pluginVersion": "4.0.7",
          "queryType": "table",
          "rawSql": "with sequences as (\n    SELECT sensor_name,\n           timestamp,\n           if(neighbor(sensor_uuid, 1, NULL) != sensor_uuid, NULL,\n              neighbor(timestamp, 1, NULL))     as next_timestamp,\n           if(neighbor(sensor_uuid, 1, NULL) != sensor_uuid, NULL,\n              neighbor(filling_value, 1, NULL)) as next_filling_value,\n           if(next_filling_value == 100 and filling_value == 100, 1, 0) as saturated\n    FROM sensors.ecological_island\n    where timestamp > $__fromTime and timestamp < $__toTime\n),\ngrouped as (\n    select sensor_name,\n           sum(if(saturated = 1, dateDiff('seconds', timestamp, next_timestamp), 0)) as total_seconds\n    from sequences\n    group by sensor_name\n)\nselect *\nfrom grouped\nwhere sensor_name = '${sensor_name}';\n",
          "refId": "A"
        }
      ],
      "title": "Total saturation",
      "transformations": [
        {
          "id": "formatTime",
          "options": {}
        }
      ],
      "type": "gauge"
